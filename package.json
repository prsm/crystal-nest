{
  "license": "MIT",
  "name": "crystal-nest",
  "description": "A Nest rewrite of crystal, the SOS Discord bot",
  "version": "0.4.8-68",
  "author": "Jonas Schultheiss <jonas.schultheiss@sos-esports.com> (https://jonasschultheiss.dev)",
  "keywords": [
    "discord",
    "nestjs",
    "prisma",
    "docker",
    "docker-compose"
  ],
  "homepage": "https://github.com/prsm/crystal-nest",
  "bugs": {
    "url": "https://github.com/prsm/crystal-nest/issues"
  },
  "private": false,
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "yarn": "^1.22.19"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prsm/crystal-nest.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start --debug 0.0.0.0:9229 --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "start:migrate:dev": "prisma migrate deploy && yarn start:debug",
    "start:migrate:prod": "prisma migrate deploy && yarn start:prod",
    "start:heroku": "npx prisma migrate deploy && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "doc:generate": "npx compodoc -p tsconfig.json -n 'go! Fleet // Versions Microservice'",
    "doc:serve": "yarn doc:generate && npx compodoc -s",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name",
    "prepare": "husky install",
    "release": "release-it"
  },
  "dependencies": {
    "@discord-nestjs/common": "4.0.8",
    "@discord-nestjs/core": "4.3.1",
    "@nestjs/common": "9.2.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.0",
    "@nestjs/platform-express": "9.2.0",
    "@prisma/client": "4.6.1",
    "bufferutil": "4.0.7",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "discord.js": "14.6.0",
    "erlpack": "discord/erlpack",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "utf-8-validate": "5.0.10",
    "zlib-sync": "0.1.7"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.5",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.2.0",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.0.3",
    "prettier": "2.8.0",
    "prisma": "4.6.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.1",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.9.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
